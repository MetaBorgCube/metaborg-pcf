module desugar

imports
  libstratego-gpp
  libstratego-sglr
  libspoofax/sdf/pp
  signatures/-

rules

  desugar-all = bottomup(try(desugar))
  
  desugar: Add(e1,e2)      -> BinExpr(e1,Plus(),e2)
  desugar: Subtract(e1,e2) -> BinExpr(e1,Minus(),e2)
  desugar: Multiply(e1,e2) -> BinExpr(e1,Times(),e2)
  desugar: Divide(e1,e2)   -> BinExpr(e1,DividedBy(),e2)
