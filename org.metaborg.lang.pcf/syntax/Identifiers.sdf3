module Identifiers

context-free syntax

  Id.Id = ID

lexical syntax

  INT        = "-"? [0-9]+

  ID         = [a-zA-Z][a-zA-Z0-9]*

lexical restrictions

  INT       -/- [0-9]

  ID        -/- [a-zA-Z0-9]

lexical syntax

  ID = Keyword {reject}
 
  Keyword = "ifz" | "then" | "else"
          | "fun"
          | "fix"
          | "let" | "in"
          | "int"
